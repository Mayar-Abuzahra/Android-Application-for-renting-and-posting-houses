<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.renthouse">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/rent"
        android:supportsRtl="true"
        android:theme="@style/Theme.RentHouse">

        <activity
            android:name=".SearchSpecificResult"
            android:exported="false" />
        <activity
            android:name=".HistoryTenant_request"
            android:exported="false" />
        <activity
            android:name=".RequestDetails"
            android:exported="false" />
        <activity
            android:name=".SearchResults"
            android:exported="false" />
        <activity
            android:name=".EditPosts"
            android:exported="false" />
        <activity
            android:name=".HomeTenant"
            android:exported="true"
            android:label="Hometenant"
            android:theme="@style/Theme.RentHouse.NoActionBar" />
        <activity
            android:name=".HomeAgency"
            android:exported="true"
            android:label="@string/title_activity_home_agency"
            android:theme="@style/Theme.RentHouse.NoActionBar" />
        <activity
            android:name=".SignUpTenant"
            android:exported="true" />
        <activity
            android:name=".SignUpAgency"
            android:exported="true" />
        <activity
            android:name=".SearchGuest"
            android:exported="true" />
        <activity
            android:name=".Entry"
            android:exported="true" />
        <activity
            android:name=".SearchGuestResults"
            android:exported="true" />
        <activity
            android:name=".SearchGuestSpecificResults"
            android:exported="true" />
        <activity
            android:name=".UserType"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>